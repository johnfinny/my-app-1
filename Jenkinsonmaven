node {
  
   
   stage('SCM Checkout'){
    // Clone repo
	git branch: 'master', 
	credentialsId: 'github', 
	url: 'https://github.com/johnfinny/my-app-1'
   }
   
   stage('Mvn Package'){
	   // Build using maven
     def mvn = tool (name: 'Maven 3', type: 'maven') + '/root/apache-maven-3.6.2/bin/mvn'
	   
	   sh "${mvn} clean package "
   }
}
